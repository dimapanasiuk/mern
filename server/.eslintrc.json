{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-var": 1,
    "no-console": 1,
    "no-alert": 1,
    "semi": [2, "always"],
    "quotes": ["error", "double"],
    "no-else-return": 0,
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-multiple-empty-lines": "error",
    "eol-last": ["error", "always"]
  }
}
